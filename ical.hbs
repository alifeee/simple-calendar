layout: ical
permalink: /calendar.ics
---
{{#each names}}
{{#each (expandcal (get .. this))}}
BEGIN:VEVENT
DTSTAMP:{{icaldatetime (now)}}
UID:{{../this}}-{{@key}}
DTSTART;VALUE=DATE:{{icaldate (parsedate @key)}}
{{!-- DTEND;VALUE=DATE:20150812 — not needed for full-day event --}}
STATUS:CONFIRMED
SUMMARY:{{../this}} – {{{icalsummary .}}}
END:VEVENT
{{/each}}{{/each}}